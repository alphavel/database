{
    "name": "alphavel/database",
    "description": "Database package for Alphavel Framework",
    "version": "1.3.0",
    "type": "library",
    "keywords": ["database", "pdo", "query-builder", "alphavel", "laravel-style", "performance"],
    "license": "MIT",
    "authors": [
        {
            "name": "Arthur Weber",
            "email": "arthur@alphavel.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "ext-pdo": "*",
        "ext-swoole": "^5.0",
        "alphavel/alphavel": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Alphavel\\Database\\": ""
        }
    },
    "extra": {
        "alphavel": {
            "providers": [
                "Alphavel\\Database\\DatabaseServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
