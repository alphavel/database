{
    "name": "alphavel/database",
    "description": "Database package with Query Builder + ORM for Alphavel Framework - #1 Fastest PHP Framework",
    "type": "library",
    "keywords": ["database", "pdo", "query-builder", "orm", "eloquent", "alphavel", "laravel-style", "performance", "swoole"],
    "license": "MIT",
    "authors": [
        {
            "name": "Arthur Weber",
            "email": "arthur@alphavel.com"
        }
    ],
    "require": {
        "php": "^8.4",
        "ext-pdo": "*",
        "ext-swoole": "^5.0",
        "alphavel/alphavel": "^1.0"
    },
    "replace": {
        "alphavel/orm": "self.version"
    },
    "autoload": {
        "psr-4": {
            "Alphavel\\Database\\": "",
            "Alphavel\\ORM\\": "ORM/"
        },
        "files": [
            "ORM/aliases.php"
        ]
    },
    "extra": {
        "alphavel": {
            "providers": [
                "Alphavel\\Database\\DatabaseServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
